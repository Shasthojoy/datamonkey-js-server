<script type="text/javascript" src="http://www.datamonkey.org/wz_tooltip.js"></script>

<SCRIPT LANGUAGE = "JavaScript" type="text/javascript">


function flagOddValues()
{
	checkValue = parseFloat (udsOptions.minread.value);
	minReadLength = udsOptions.min.value;
	maxReadLength = udsOptions.max.value;
	

	if (checkValue > maxReadLength)
	{
		alert ("Warning: Your minimum read length specified is greater than the maximum of all reads. No data will be processed.");
	}
	if ( checkValue < minReadLength )
	{
		alert ("Warning: Your minimum read length specified is less than the minimum of all reads. All data will be processed." );
	}
	
}




</SCRIPT>