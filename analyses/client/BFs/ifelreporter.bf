ExecuteAFile("../Shared/HyPhyGlobals.ibf");ExecuteAFile("../Shared/GrabBag.bf");fscanf		(stdin,"String", filePrefix);fscanf		(stdin,"Number", optionOutput);if (optionOutput >= 4){	fscanf		(stdin,"String", gnuplotFormat);	fscanf		(stdin,"String", style);	fscanf		(stdin,"String", xaxis);	fscanf		(stdin,"String", yaxis);	fscanf		(stdin,"String", size);} /* ________________________________________________________________________________________________*/function ErrorOut (errString){	fprintf (stdout, "ERROR:<DIV class = 'ErrorTagSM'>\n", errString, "\n</DIV>");	return 0;}/* ________________________________________________________________________________________________*/if (optionOutput!=2){	ExecuteAFile	("../Shared/DBTools.ibf");	slacDBID 		 = _openCacheDB      (filePrefix);	pv = _ExecuteSQL  (slacDBID,"SELECT COL_VALUE FROM IFEL_SUMMARY WHERE COL_KEY = 'PValue'");	pv = 0+pv[0];	ExecuteAFile 			("../Shared/OutputsIFEL.bf");	tableKeys = Rows		(IFEL_ResultTable);}if (optionOutput < 2){	generalInfo = _ExecuteSQL  (slacDBID,"SELECT * FROM IFEL_RESULTS ORDER BY CODON");		titleMatrix 	= {1,10};	titleMatrix[0]  = "Codon";	titleMatrix[1]  = "dS";	titleMatrix[2]  = "dN";	titleMatrix[3]  = "dN Leaves";	titleMatrix[4]  = "dN/dS";	titleMatrix[5]  = "Normalized dN-dS";	titleMatrix[6]  = "dS (when dN=dS)";	titleMatrix[7]  = "Log(L)";	titleMatrix[8]  = "LRT";	titleMatrix[9]  = "p-value";	rowCount  = Abs	   		(generalInfo);	colCount  = Abs	        (IFEL_ResultTable);		colMap	  = {{0,1,2,3,-1,8,4,5,6,7}};		IFELInfo = {rowCount, colCount};		for (r=0; r<rowCount; r=r+1)	{		for (c=0; c<colCount; c=c+1)		{			IFELInfo[r][c] = 0+(generalInfo[r])[tableKeys[c]]; 		}	}	IFELInfo = IFELInfo % 0;		if (optionOutput == 1) /* CSV */	{		fprintf (stdout, titleMatrix[0]);		for (r=1; r<colCount; r=r+1)		{			fprintf (stdout, ",", titleMatrix[r]);		}				for (r=0; r<rowCount; r=r+1)		{			fprintf (stdout, "\n", IFELInfo[r][0]+1);			for (c=1; c<=colCount; c=c+1)			{				if (c==4)				{					fprintf (stdout, ",", _normalizeRatio (IFELInfo[r][1],IFELInfo[r][2]));				}				else				{					fprintf (stdout, ",", IFELInfo[r][colMap[c]]);				}			}		}	}	else	{		fprintf (stdout, "<H1 CLASS = 'SuccessCap'>Detailed IFEL results</H1>");		fprintf (stdout, _makeJobIDHTML (filePrefix));		fprintf (stdout, "<DIV CLASS = 'RepClassSM'>Detailed analysis results (see legend at the bottom of the page)");						fprintf (stdout, "<TABLE BORDER = '0' style = 'margin:10px'><TR class = 'TRReportT'>");		for (r=0; r<=colCount; r=r+1)		{			fprintf (stdout, "<TD>", titleMatrix[r], "</TD>");		}		fprintf (stdout, "</TR>\n");		for (r=0; r<rowCount; r=r+1)		{			trClass = "TRReportNT";			if (IFELInfo[r][1]+IFELInfo[r][2] > 0)			{				if (IFELInfo[r][colCount-2]<=pv)				{					if (IFELInfo[r][1]<IFELInfo[r][2])					{						trClass = "TRReportPS";								}					else					{						trClass = "TRReportNS";					}				}			}			fprintf (stdout, "<TR class = '",trClass,"' style = 'font-size:9px;'>");			fprintf (stdout, "<TD>", r+1, "</TD>");			for (c=1; c<=colCount; c=c+1)			{				if (c==4)				{					fprintf (stdout, "<TD>",_normalizeRatio (IFELInfo[r][1],IFELInfo[r][2]), "</TD>");				}				else				{					if (IFELInfo[r][colMap[c]]$1 != IFELInfo[r][colMap[c]])					{						fprintf (stdout, "<TD>", Format (IFELInfo[r][colMap[c]],5,2), "</TD>");					}					else					{						fprintf (stdout, "<TD>", IFELInfo[r][colMap[c]], "</TD>");									}				}			}			fprintf (stdout, "</TR>\n");		}		fprintf (stdout, "</TABLE>");						fscanf ("../Formats/IFEL_report","Raw",IFEL_Legend);		fprintf (stdout, IFEL_Legend);		fprintf (stdout, "</DIV>");	}}else{	if (optionOutput == 2)	{		fprintf (stdout, "<H1 CLASS = 'SuccessCap'>Generate selection plots from IFEL results</H1>");		fprintf (stdout, _makeJobIDHTML (filePrefix));		fprintf (stdout, "<FORM method='POST' name = 'plotForm' enctype='multipart/form-data' action='",BASE_CGI_URL_STRING,"rungnuplot.pl' target = '_blank'>\n<input type = 'hidden' value = '",filePrefix,"' name = 'inFile'><input type = 'hidden' value = '2' name = 'task'>");		fscanf  ("../Formats/IFELplot","Raw",IFELplot);		fprintf (stdout, IFELplot, "</form>");	}	else	{		fprintf (stdout, "set term ", gnuplotFormat);		if (gnuplotFormat == "png")		{			fprintf (stdout, " ", size);		}		fprintf (stdout, "\nset output\nset nokey\nset xlabel '", xaxis, "'\nset ylabel '",yaxis, "'\nplot '-' lt -1 with ",style,"\n");		pv = _ExecuteSQL  (slacDBID,"SELECT "+ tableKeys[optionOutput-4] +" FROM IFEL_RESULTS ORDER BY Codon");		if (optionOutput == 12)		{			for (k=0; k<Abs(pv); k=k+1)			{				fprintf (stdout, "\n", k+1, "\t", Max(-100,Min(0+pv[k],100)));			}				}		else		{			for (k=0; k<Abs(pv); k=k+1)			{				fprintf (stdout, "\n", k+1, "\t", pv[k]);			}		}	}}if (optionOutput!=2){	_closeCacheDB (slacDBID);}