ExecuteAFile ("../../Shared/HyPhyGlobals.ibf");ExecuteAFile ("../../Shared/GrabBag.bf");ExecuteAFile ("../../Shared/ReadDelimitedFiles.bf");ExecuteAFile ("../../Shared/DBTools.ibf");fscanf		 (stdin, "String", arg);arg = splitOnRegExp (arg,"-");if (Abs (arg) < 2){	defaultErrorOut ("Argument Error");	return 0;}slacDBID 		 = _openCacheDB      (arg[0]);haveScueal 	     = _TableExists (slacDBID, "SUBTYPING_RESULTS");if (haveScueal){	inClause = ""; inClause * 128;	inClause * ("(" + arg[1]); 	for (k = 2; k < Abs(arg); k = k+1)	{		inClause * ("," + arg[k]);	}	inClause * ")"; inClause * 0;	DoSQL (slacDBID, "SELECT ID, SIMPLIFIED, SEQUENCE FROM SUBTYPING_RESULTS WHERE FILE_INDEX IN " + inClause + " ORDER BY SIMPLIFIED,FILE_INDEX","printASequence()");	}else{	defaultErrorOut ("No SCUEAL results for this file");}_closeCacheDB (slacDBID);function printASequence (){	fprintf (stdout, ">", SQL_ROW_DATA[0], "[", SQL_ROW_DATA[1], "]\n", SQL_ROW_DATA[2],"\n");	return 0;}