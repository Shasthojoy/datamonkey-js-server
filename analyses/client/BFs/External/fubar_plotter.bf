ExecuteAFile ("../../Shared/HyPhyGlobals.ibf");ExecuteAFile ("../../Shared/GrabBag.bf");ExecuteAFile ("../../Shared/PostScript.bf");ExecuteAFile ("../../Shared/ReadDelimitedFiles.bf");ExecuteAFile ("../../Shared/DBTools.ibf");ExecuteAFile ("../../Shared/OutputsFUBAR.bf");fscanf		 (stdin, "String", arg);arg = splitOnRegExp (arg,"-");if (Abs (arg) < 3){	fprintf (stdout, "");	return 0;}slacDBID 		 = _openCacheDB      (arg[0]);if (arg[1] == "grid") {    siteID = -1;} else {    siteID           = 0 + arg[1];}format           = arg[2];if (siteID == 0) {    fprintf (stdout, report3DGrid (slacDBID, _ExecuteSQL (slacDBID, "SELECT alpha,beta,weight from FUBAR_GRID"), _ExecuteSQL (slacDBID, "SELECT alpha,beta,logl from FUBAR_GRID"), format, 1, 0));} else {    if (siteID < 0) {        fprintf (stdout, report3DGrid (slacDBID, _ExecuteSQL (slacDBID, "SELECT alpha,beta,weight from FUBAR_GRID"), _ExecuteSQL (slacDBID, "SELECT alpha,beta,logl from FUBAR_GRID"), format, 1, 1));    } else {        fprintf (stdout, report3DGrid (slacDBID, _ExecuteSQL (slacDBID, "SELECT alpha,beta,weight from FUBAR_SITE_INFO where codon == " + siteID), _ExecuteSQL (slacDBID, "SELECT alpha,beta,prob from FUBAR_SITE_INFO where codon == " + siteID), format,1,0));    }}_closeCacheDB (slacDBID);