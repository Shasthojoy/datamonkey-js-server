ExecuteAFile	("../Shared/HyPhyGlobals.ibf");ExecuteAFile	("../Shared/GrabBag.bf");ExecuteAFile	("../Shared/OutputsIFEL.bf");fscanf		(stdin,"String", filePrefix);fscanf		(stdin,"Number", sigLevel);sigLevel = Max (0,sigLevel);sigLevel = Min (1,sigLevel);/* ________________________________________________________________________________________________*/function ErrorOut (errString){	fprintf (stdout, "ERROR:<DIV class = 'ErrorTagSM'>\n", errString, "\n</DIV>");	return 0;}/* ________________________________________________________________________________________________*/ExecuteAFile	("../Shared/DBTools.ibf");slacDBID 		 = _openCacheDB      (filePrefix);ps = _ExecuteSQL  (slacDBID, "SELECT Codon,dS,dN,dN_Leaves,p,ScaledDNDS FROM IFEL_RESULTS WHERE (dS<dN AND p<="+sigLevel+") ORDER BY Codon");ns = _ExecuteSQL  (slacDBID, "SELECT Codon,dS,dN,dN_Leaves,p,ScaledDNDS FROM IFEL_RESULTS WHERE (dS>dN AND p<="+sigLevel+") ORDER BY Codon");posSelected= Abs(ps);negSelected= Abs(ns);keyMap = {{"Codon","dS","dN","dN_Leaves","ScaledDNDS","p"}};doSLACM = _TableExists (slacDBID, "SLAC_SUMMARY");_closeCacheDB (slacDBID);fprintf (stdout, "<H1 class = 'SuccessCap'>Selected sites using IFEL</H1>", _makeJobIDHTML (filePrefix));fprintf (stdout, "<DIV CLASS='RepClassSM'><form action='",BASE_CGI_URL_STRING,"slacpvalue.pl' method = 'GET'>Found <b>",posSelected,				   "</b> positively selected sites (<input type = 'hidden' name = 'file' value = '",				   filePrefix,"'><input type = 'text' name = 'pvalue' value = '",				   sigLevel,				   "' size = '5'> significance level <input type = 'hidden' value = '2' name = 'task'><input type = 'submit' value = 'Retabulate'>)<p>",PrintASCIITable  (AVL2Matrix(ps,keyMap), selLabelMatrix,doSLACM),"</form></DIV>");fprintf (stdout, "<DIV CLASS='RepClassSM'>Found <b>",negSelected,				   "</b> negatively selected sites (", sigLevel," significance level)<p>",PrintASCIITable  (AVL2Matrix(ns,keyMap), selLabelMatrix,doSLACM),"</form></DIV>");