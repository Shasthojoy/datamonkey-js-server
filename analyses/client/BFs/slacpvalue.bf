ExecuteAFile	("../Shared/HyPhyGlobals.ibf");ExecuteAFile	("../Shared/GrabBag.bf");ExecuteAFile	("../Shared/Outputs.bf");fscanf		(stdin,"String", filePrefix);fscanf		(stdin,"Number", sigLevel);sigLevel = Max (0,sigLevel);sigLevel = Min (1,sigLevel);/* ________________________________________________________________________________________________*/function ErrorOut (errString){	fprintf (stdout, "ERROR:<DIV class = 'ErrorTagSM'>\n", errString, "\n</DIV>");	return 0;}/* ________________________________________________________________________________________________*/ExecuteAFile	("../Shared/DBTools.ibf");slacDBID 		 = _openCacheDB      (filePrefix);ps = _ExecuteSQL  (slacDBID,"SELECT FIELD_0+1 AS FIELD_0,FIELD_9,FIELD_12,FIELD_10 FROM SLAC_RESULTS WHERE FIELD_9>0 AND FIELD_10<="+sigLevel);ns = _ExecuteSQL  (slacDBID,"SELECT FIELD_0+1 AS FIELD_0,FIELD_9,FIELD_12,FIELD_11 FROM SLAC_RESULTS WHERE FIELD_9<0 AND FIELD_11<="+sigLevel);posSelected= Abs(ps);negSelected= Abs(ns);_closeCacheDB (slacDBID);fprintf (stdout, "<H1 class = 'SuccessCap'>Selected sites using SLAC</H1>", _makeJobIDHTML (filePrefix));fprintf (stdout, "<DIV CLASS='RepClassSM'><form action='",BASE_CGI_URL_STRING,"slacpvalue.pl' method = 'GET'>Found <b>",posSelected,				   "</b> positively selected sites (<input type = 'hidden' name = 'file' value = '",				   filePrefix,"'><input type = 'text' name = 'pvalue' value = '",				   sigLevel,				   "' size = '5'> significance level <input type = 'submit' value = 'Retabulate'>)<p>",PrintASCIITable  (AVL2Matrix(ps,{{"FIELD_0","FIELD_9","FIELD_12","FIELD_10"}}), selLabelMatrix),"</form></DIV>");fprintf (stdout, "<DIV CLASS='RepClassSM'>Found <b>",negSelected,				   "</b> negatively selected sites (", sigLevel," significance level)<p>",PrintASCIITable  (AVL2Matrix(ns,{{"FIELD_0","FIELD_9","FIELD_12","FIELD_11"}}), selLabelMatrix),"</form></DIV>");