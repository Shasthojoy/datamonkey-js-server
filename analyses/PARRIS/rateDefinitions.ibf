function defineAlpha (dummy){    global PS1  = 1/3;    global PS2  = 1/2;    global RS1  = 1/3;    global IRS3 = 2/3;    global RS3 := 1/IRS3;	PS1:<1;	PS2:<1;	RS1:<1; RS1:>0.000000001;	IRS3:<1;	global synRate_scale := RS1*PS1 + (1-PS1)*PS2 + RS3*(1-PS1)*(1-PS2);	categFreqMatrix = {{PS1,(1-PS1)*PS2, (1-PS1)*(1-PS2)}} ;	categRateMatrix = {{RS1/synRate_scale,1/synRate_scale,RS3/synRate_scale}};	category c = (3, categFreqMatrix , MEAN, ,categRateMatrix, 0, 1e25);				return 0;}function defineBeta (nullOrAlt){	if (nullOrAlt)	{        PS1  		 = 1/3;   		PS2  		 = 1/2;    	RS1  		 = 1/3;	    IRS3 		 = 2/3;		P1			 = 0.5;		W_1			 = 0.25;	    global P2    = 0.9;	    global W_2_I = 0.2;	    P2:<1; 		W_2_I :< 1; W_2_I:>0.05;	    global W_2  := 1/W_2_I;		categFreqMatrix = {{P1,(1-P1)*P2, (1-P1)*(1-P2)}} ;		categRateMatrix = {{W_1,1,W_2}};		category d      = (3, categFreqMatrix , MEAN, ,categRateMatrix, 0, 1e25);	}	else	{	    global P1 =  0.8;	    global W_1 = .25;		P1:<1;		W_1 :< 1; W_1:>0.000000001;		categFreqMatrix = {{P1,1-P1}} ;		categRateMatrix = {{W_1,1}};		category d = (2, categFreqMatrix , MEAN, ,categRateMatrix, 0, 1e25);					}				return 0;}